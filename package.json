{
  "name": "react-es6-isomorphic",
  "version": "0.7.0",
  "description": "A simple app to try React and ES6, using topheman-apis-proxy as data api backend NOW with isomorphism",
  "main": "index.js",
  "scripts": {
    "start": "node ./server/bin/www",
    "nodemon": "node_modules/nodemon/bin/nodemon.js ./server/bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server-dev": "NODE_ENV=development DEBUG=http,errors npm run nodemon",
    "server-mock": "NODE_ENV=MOCK DEBUG=http,errors npm run nodemon",
    "server-prod": "NODE_ENV=PROD DEBUG=http,errors npm run nodemon",
    "webpack-dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors",
    "webpack-mock": "MOCK=true ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors",
    "webpack-build": "grunt clean:build & ./node_modules/webpack/bin/webpack.js --config ./webpack.config.js",
    "webpack-build-prod": "grunt clean:build & PROD=true ./node_modules/webpack/bin/webpack.js -p --config ./webpack.config.js",
    "postinstall": "npm run webpack-build-prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/topheman/react-es6.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Christophe Rosset",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/topheman/react-es6/issues"
  },
  "homepage": "https://github.com/topheman/react-es6",
  "engines": {
    "node": "0.12.0"
  },
  "devDependencies": {
    "babel-core": "^4.7.16",
    "babel-loader": "^4.2.0",
    "body-parser": "~1.12.0",
    "cookie-parser": "~1.3.4",
    "css-loader": "^0.9.1",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "extract-text-webpack-plugin": "^0.5.0",
    "file-loader": "^0.8.1",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "jade": "~1.9.2",
    "json-loader": "^0.5.1",
    "load-grunt-tasks": "^3.1.0",
    "morgan": "~1.5.1",
    "node-sass": "^3.0.0-alpha.0",
    "nodemon": "^1.3.7",
    "react-hot-loader": "^1.2.4",
    "run-sequence": "^1.0.2",
    "sass-loader": "^1.0.0",
    "serve-favicon": "~2.2.0",
    "style-loader": "^0.9.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.7.3",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "babel": "^5.1.10",
    "bootstrap-sass": "^3.3.4",
    "ejs": "^2.3.1",
    "express-ejs-layouts": "^1.1.0",
    "lscache": "^1.0.5",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "superagent": "^1.1.0"
  },
  "private": true
}
